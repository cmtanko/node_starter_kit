{
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "verbose":true,
    "moduleDirectories": [
      "node_modules",
      "src/server"
    ],
    "collectCoverageFrom": [
      "{src}/**/*.{ts}"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "(.mock)\\.(js)$"
    ],
    "transform": {
        ".*(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
      },
    "testMatch": [
      "**/*/*.test.(ts|tsx|js)"
    ],
    "coverageDirectory": "../../coverage/server",
    "cacheDirectory": "/var/tmp/jest_cache",
    "coverageReporters": [
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 2.37,
        "statements": 2.26,
        "functions": 3.45,
        "branches": 0,
        "statments": 0
      }
    }
  }
  