{
  "name": "node_starter_kit",
  "version": "1.0.0",
  "description": "Backend API with node , starter kit",
  "main": "dist/api.js",
  "scripts": {
    "preinstall": "npm install pm2 -g",
    "migrate": "knex migrate:latest --knexfile=dist/knexfile.js",
    "seed": "knex seed:run --knexfile=dist/knexfile.js",
    "compile": "./node_modules/.bin/tsc",
    "start": "pm2 start dist/api.js -i max --attach",
    "lint": "eslint src && npm run test",
    "codecov": "cat coverage/lcov.info | codecov",
    "precommit": "npm run lint --silent",
    "build": "babel -d ./dist ./src -s",
    "dev": "nodemon --exec ./node_modules/.bin/ts-node -- src/api.ts",
    "stop": "pm2 stop api",
    "test": "npm run coverage",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha --reporter lcovonly -- test/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmtanko/node_starter_kit.git"
  },
  "author": "Suchan Badyakar",
  "license": "ISC",
  "dependencies": {
    "@types/swagger-jsdoc": "0.0.2",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.17.2",
    "bookshelf": "^0.10.4",
    "bookshelf-manager": "^0.3.0",
    "boom": "^7.1.1",
    "chai": "^4.1.2",
    "cors": "^2.8.4",
    "deep-object-graph": "^1.1.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "helmet": "^3.8.1",
    "import": "0.0.6",
    "knex": "^0.14.0",
    "lodash": "^4.17.4",
    "mocha": "^3.5.0",
    "morgan": "^1.8.2",
    "nodemailer": "^4.0.1",
    "path": "^0.12.7",
    "pg": "^7.4.0",
    "sinon": "^3.2.1",
    "swagger-jsdoc": "^1.3.0",
    "treeize": "^2.1.2",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.18",
    "@types/body-parser": "^1.16.8",
    "@types/bookshelf": "^0.9.7",
    "@types/boom": "^4.3.8",
    "@types/cors": "^2.8.3",
    "@types/express": "^4.0.39",
    "@types/helmet": "0.0.37",
    "@types/knex": "0.0.64",
    "@types/lodash": "^4.14.85",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.0.51",
    "@types/swagger-jsdoc": "0.0.2",
    "@types/winston": "^2.3.7",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-watch": "^2.0.7",
    "codecov": "^3.0.0",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jasmine": "^2.8.4",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
