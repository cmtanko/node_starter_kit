{
  "name": "node_starter_kit",
  "version": "1.0.0",
  "description": "Backend API with node , starter kit",
  "main": "dist/server.js",
  "scripts": {
    "migrate": "knex migrate:latest --knexfile=dist/knexfile.js",
    "seed": "knex seed:run --knexfile=dist/knexfile.js",
    "start": "node dist/server.js",
    "stop": "pm2 stop api",
    "lint": "eslint src && npm run test",
    "build": "./node_modules/.bin/tsc",
    "dev": "nodemon --exec ./node_modules/.bin/ts-node -- src/server.ts",
    "test": "npm run test:jest",
    "test:jest": "jest --config jest-config.json --bail",
    "test:coverage": "jest --config jest-config.json --coverage --bail"
  },
